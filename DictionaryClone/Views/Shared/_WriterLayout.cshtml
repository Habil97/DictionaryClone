
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="~/DictionaryTheme/assets/img/apple-icon.png">
    <link rel="icon" type="image/png" href="../assets/img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>YAZAR PANELİ</title>

    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport' />
    <link href="https://fonts.googleapis.com/css?family=Montserrat:400,700,200" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" />
    <link href="~/DictionaryTheme/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/DictionaryTheme/assets/css/light-bootstrap-dashboard.css?v=2.0.0 " rel="stylesheet" />
    <link href="~/DictionaryTheme/assets/css/demo.css" rel="stylesheet" />
</head>

<body>
    <div class="wrapper">
        <div class="sidebar" data-image="~/DictionaryTheme/assets/img/sidebar-5.jpg">

            <div class="sidebar-wrapper">
                <div class="logo">
                    <a class="simple-text">
                       YAZAR PANELİ
                    </a>
                </div>
                <ul class="nav">
                    <li>
                        <a class="nav-link" href="/WriterPanel/WriterProfile">
                            <i class="nc-icon nc-chart-pie-35"></i>
                            <p>Profilim</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/WriterPanel/MyHeading">
                            <i class="nc-icon nc-align-left-2"></i>
                            <p>Başlıklarım</p>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="/WriterPanel/AllHeading">
                            <i class="nc-icon nc-album-2"></i>
                            <p>Tüm Başlıklar</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/WriterPanelMessage/Inbox">
                            <i class="nc-icon nc-bell-55"></i>
                            <p>Mesajlar</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/WriterPanelContent/MyContent">
                            <i class="nc-icon nc-atom"></i>
                            <p>Yorumlarım</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/ShowCase/Index">
                            <i class="nc-icon nc-tv-2"></i>
                            <p>Siteye Git</p>
                        </a>
                    </li>
                    <li>
                        <a class="nav-link" href="/WriterLogin/LogOut">
                            <i class="nc-icon nc-send"></i>
                            <p>Çıkış Yap</p>
                        </a>
                    </li>

                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg " color-on-scroll="500">
                <div class="container-fluid">
                    <button href="" class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                        <span class="navbar-toggler-bar burger-lines"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end" id="navigation">
                    </div>
                </div>
            </nav>

            <div class="content">
                @RenderBody()
            </div>

            <footer class="footer">
                <div class="container-fluid">
                    <nav>
                        <ul class="footer-menu">
                        </ul>
                        <p class="copyright text-center">
                            ©
                            <script>
                                document.write(new Date().getFullYear())
                            </script>
                            <a>Tüm Hakları Saklıdır.</a>
                        </p>
                    </nav>
                </div>
            </footer>
        </div>
    </div>

    <script src="~/DictionaryTheme/assets/js/core/jquery.3.2.1.min.js" type="text/javascript"></script>
    <script src="~/DictionaryTheme/assets/js/core/popper.min.js" type="text/javascript"></script>
    <script src="~/DictionaryTheme/assets/js/core/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/DictionaryTheme/assets/js/plugins/bootstrap-switch.js"></script>
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE"></script>
    <script src="~/DictionaryTheme/assets/js/plugins/chartist.min.js"></script>
    <script src="~/DictionaryTheme/assets/js/plugins/bootstrap-notify.js"></script>
    <script src="~/DictionaryTheme/assets/js/light-bootstrap-dashboard.js?v=2.0.0 " type="text/javascript"></script>
    <script src="~/DictionaryTheme/assets/js/demo.js"></script>

</body>
</html>
